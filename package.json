{
  "publisher": "deuscx",
  "name": "pkg-name",
  "displayName": "pkg-name",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.10.5",
  "description": "",
  "license": "MIT",
  "funding": "https://github.com/sponsors/deuscx",
  "homepage": "https://github.com/deuscx/pkg-name#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/deuscx/pkg-name"
  },
  "bugs": {
    "url": "https://github.com/deuscx/pkg-name/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/deuscx"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.83.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "pkg-name.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.1",
    "@types/node": "^20.9.0",
    "@types/vscode": "^1.84.1",
    "@vscode/vsce": "^2.22.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.1.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
  }
}
