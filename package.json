{
  "publisher": "deuscx",
  "name": "pkg-name",
  "displayName": "pkg-name",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "description": "",
  "license": "MIT",
  "funding": "https://github.com/sponsors/deuscx",
  "homepage": "https://github.com/deuscx/pkg-name#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/deuscx/pkg-name"
  },
  "bugs": {
    "url": "https://github.com/deuscx/pkg-name/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.78.1"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "pkg-name.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@types/node": "^18.16.16",
    "@types/vscode": "^1.78.1",
    "@vscode/vsce": "^2.19.0",
    "bumpp": "^9.1.0",
    "eslint": "^8.42.0",
    "rimraf": "^5.0.1",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "vite": "^4.2.3",
    "vitest": "^0.31.4"
  }
}
